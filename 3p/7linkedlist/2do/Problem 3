class  Add{
		constructor(){
    		this.arreglo=[1,2,3,4];
      	this.arreglo2=[];
      	this.arreglo3=[]; 	
    
    };
  	
  	eliminar() {
        let p=3;
    		for(let i=0;i<this.arreglo.length;i++){
					 	 if(i==p){
   						 this.arreglo3.push(this.arreglo[i]);
             }else{
   						   this.arreglo2.push(this.arreglo[i]);
               }
  			 }
        return this.arreglo2;
    };
};
  
const add= new Add();
console.log(add);
console.log(add.eliminar());
